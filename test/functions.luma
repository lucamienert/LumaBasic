func myFunction() => none
{
    sysout("Test from function");
};

func myFunctionWithParameter(int x) => none
{
    sysout(x);
};

func myFunctionWithParameters(string y, string z) => none
{
    sysout(y);
    sysout(z);
};

func main() => int
{
    myFunction();
    myFunctionWithParameter(10);
    myFunctionWithParameters("20", "30");
    return 0;
};